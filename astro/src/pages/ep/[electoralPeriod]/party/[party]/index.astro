---
import Layout from '../../../../../layouts/Layout.astro';
import type { InferGetStaticPropsType, InferGetStaticParamsType, GetStaticPaths } from 'astro';

export const getStaticPaths = (async () => {
  return [
    { params: { electoralPeriod: 'magdeburg-7', party: 'party-1' }},
    { params: { electoralPeriod: 'magdeburg-7', party: 'party-2' }},
    { params: { electoralPeriod: 'magdeburg-8', party: 'party-1' }},
    { params: { electoralPeriod: 'magdeburg-8', party: 'party-2' }}
  ];
}) satisfies GetStaticPaths;

type Params = InferGetStaticParamsType<typeof getStaticPaths>;
type Props = InferGetStaticPropsType<typeof getStaticPaths>;

const { electoralPeriod, party } = Astro.params as Params;

---
<Layout>
  Electoral Period { electoralPeriod }<br>
  Party { party }
</Layout>
