---
import Layout from '../../../../layouts/Layout.astro';
import type { InferGetStaticPropsType, InferGetStaticParamsType, GetStaticPaths } from 'astro';

export const getStaticPaths = (async () => {
  return [
    { params: { electoralPeriod: 'magdeburg-7' }},
    { params: { electoralPeriod: 'magdeburg-8' }}
  ];
}) satisfies GetStaticPaths;

type Params = InferGetStaticParamsType<typeof getStaticPaths>;
type Props = InferGetStaticPropsType<typeof getStaticPaths>;

const { electoralPeriod } = Astro.params as Params;

---
<Layout>
  Factions of Electoral Period { electoralPeriod }
</Layout>
